<nav class="header__nav">
    <div class="header__left">
        <div class="header__left__int">
            {{#if @site.logo}}
                {{!-- <a class="site-nav-logo" href="{{@site.url}}"><img src="{{@site.logo}}" alt="{{@site.title}}" /></a> --}}
                <a class="header__logo" href="{{@site.url}}"><img src="{{asset 'images/logo-prixtips.png'}}" alt="{{@site.title}}" /></a>
            {{else}}
                <a class="site-nav-logo" href="{{@site.url}}">{{@site.title}}</a>
            {{/if}}
        </div>
    </div>
    <div class="header__middle">
        <div class="menu__content">
            {{#if @site.navigation}}
                {{navigation}}
            {{/if}}
            {{#is "post"}}
                <span class="nav-post-title {{#unless @site.logo}}dash{{/unless}}">{{post.title}}</span>
            {{/is}}
        </div>
    </div>
    <div class="header__right">
        {{#if @site.secondary_navigation}}
            {{navigation type="secondary"}}
        {{else}}
            <div class="socials">
                {{#if @site.facebook}}
                    <a class="socials__link socials__link--fb" href="{{facebook_url @site.facebook}}" title="Facebook" target="_blank" rel="noopener">{{> "icons/facebook"}}</a>
                {{/if}}
                {{#if @site.twitter}}
                    <a class="socials__link socials__link--fb" href="{{twitter_url @site.twitter}}" title="Twitter" target="_blank" rel="noopener">{{> "icons/twitter"}}</a>
                {{/if}}
            </div>
            {{!-- {{#unless @labs.members}}
                <a class="rss-button" href="https://feedly.com/i/subscription/feed/{{@site.url}}/rss/" title="RSS" target="_blank" rel="noopener">{{> "icons/rss"}}</a>
            {{/unless}} --}}
        {{/if}}

        {{!-- {{#if @labs.members}}
            <a class="subscribe-button" href="#subscribe">Subscribe</a>
        {{/if}} --}}
    </div>
</nav>
